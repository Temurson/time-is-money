{
    "name": "Time Is Money",
    "version": "0.0.1",
    "description": "An extension that helps you manage your time on YouTube.",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab"
    ],
    "page_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html"
}